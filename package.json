        "eslint-plugin-simple-import-sort": "^8.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "postcss": "^8.4.21",
        "sass": "^1.58.0",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.9.3",
        "vite": "^4.1.0",
        "vite-plugin-wasm-pack": "^0.1.12"
